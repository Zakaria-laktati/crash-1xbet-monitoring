  session: VOTRE_SESSION_ID_ICI
  access_token: VOTRE_ACCESS_TOKEN_ICI
  account_id: VOTRE_ACCOUNT_ID_ICI

scraper:
  source: 1xbet
  websocket:
    base_url: wss://ma-1xbet.com/games-frame/sockets/crash
    params:
      ref: 1
      gr: 1529
      whence: 55
      fcountry: 125
      appGuid: games-web-host-b2c-web-v3
      lng: fr
      v: '1.5'
